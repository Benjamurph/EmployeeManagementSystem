@model IEnumerable<ApplicationUser>

@{

    ViewData["Title"] = "User Profile";
}
<div class="row justify-content-center" style="padding-top: 50px">
<div class="col-lg-4">
<div class="card card-primary card-outline" style="border-top: 3px solid #3d66a9">
    <div class="card-body box-profile" >
        <div class="text-center">
            <img class="profile-user-img img-fluid img-circle" style="max-height:100px"
                         src=@ViewData["UserProfilePicture"]>
        </div>

        <h3 class="profile-username text-center">@ViewData["UserFullName"]</h3>




                <a href="#" class="btn btn-primary btn-block" style="background-color: #3d66a9; border: #3d66a9; margin-top: 20px"><b>Change Profile Picture</b></a>
        <a href="#" class="btn btn-primary btn-block" style="background-color: #3d66a9; border: #3d66a9; margin-top: 20px"><b>Reset Password</b></a>
    </div>
    </div>
    </div>
    </div>